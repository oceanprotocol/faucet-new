{
  
    "extends": ["eslint:recommended", "prettier"],
    "env": { "es6": true, "browser": true, "node": true, "jest": true },
    "overrides": [
      {
        "files": ["**/*.js"],
        "extends": [
          "plugin:prettier/recommended",
          "prettier"
        ],
        "plugins": ["prettier"],
        "rules": {
          "no-use-before-define": "off"
        }
      }
    ]
  }